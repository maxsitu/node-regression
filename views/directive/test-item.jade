doctype html
div.test-item(ng-class="{success: !itemNode.fail, failure: itemNode.fail, running: isRunning()}")
    div(ng-if="!itemNode.children" )
        a(ng-click="select(itemNode)") {{itemNode.label}}

    div(ng-if="itemNode.children || !itemNode.cmd")
        a(ng-click="select(itemNode)") {{itemNode.label}}
